<#Mandy-Binning>
    cb:name "Mandy Binning" ; 
    cb:name "Binning" ; 
    cb:property "faculty" ; 
    cb:title "Assistant Professor; Stroke Director" ; 
    cb:department "Neurosurgery" ; 
